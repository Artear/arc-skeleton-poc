{
  "name": "PageBuilder-Fusion-Features",
  "version": "1.0.0",
  "description": "PageBuilder Fusion features",
  "scripts": {
    "build": "npm run clean && webpack --config ./config/webpack.base.js --env.prod",
    "clean": "node scripts/clean.js",
    "dev": "npm run clean && webpack --watch --config ./config/webpack.base.js --env.dev --progress --debug",
    "lint": "npm run eslint && npm run stylelint",
    "stylelint": "stylelint 'src/**/*.scss'",
    "eslint": "eslint --ext=js,jsx .",
    "start": "fusion start",
    "test": "jest --coverage",
    "build:plugins": "sh scripts/build-plugins.sh",
    "watch:plugins": "sh scripts/watch-plugins.sh"
  },
  "devDependencies": {
    "@arc-fusion/cli": "^1.0.3",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "amphtml-validator": "^1.0.23",
    "autoprefixer": "^9.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-root-import": "^6.1.0",
    "babel-polyfill": "^6.26.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "css-mqpacker": "^7.0.0",
    "cypress": "^3.1.5",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-import-resolver-babel-root-import": "0.0.2",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.8.2",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "husky": "^2.1.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "lint-staged": "^8.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "node-sass-glob-importer": "5.3.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-test-renderer": "^16.6.1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-prettier": "^1.0.6",
    "stylelint-webpack-plugin": "^0.10.5",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  },
  "author": "Arc Publishing",
  "license": "ISC",
  "dependencies": {
    "@arc-core-components/amp-document-boilerplate": "^0.2.5-beta.1",
    "@arc-core-components/feature_ads-arc-ad": "^0.2.3-beta.5",
    "@arc-core-components/feature_global-amp-gallery": "^0.3.1-beta.6",
    "@arc-core-components/feature_global-amp-nav": "^0.3.2-beta.4",
    "fecha": "^3.0.0",
    "lazy-child": "^0.1.0"
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,graphql}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "plugin:react/recommended",
      "plugin:jest/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "settings": {
      "react": {
        "version": "16.6.3"
      }
    },
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": [
        ".js",
        ".jsx",
        ".json",
        ".css",
        ".scss",
        ".md"
      ],
      "arrow-body-style": [
        "error",
        "always"
      ],
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "max-len": [
        "error",
        140
      ],
      "no-underscore-dangle": "off",
      "unicorn/filename-case": "off"
    },
    "overrides": [
      {
        "files": [
          "scripts/*",
          "config/**"
        ],
        "rules": {
          "no-console": "off"
        }
      },
      {
        "files": [
          "**/*.test.js"
        ],
        "rules": {
          "no-unused-vars": [
            "error",
            {
              "varsIgnorePattern": "Consumer"
            }
          ]
        }
      }
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85
      }
    },
    "moduleDirectories": [
      "node_modules",
      "components",
      "mocks",
      "properties"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "moduleNameMapper": {
      "fusion:properties": "<rootDir>/jest/fusionProperties.js"
    }
  },
  "main": "setupTests.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wapopartners/Skeleton-PageBuilder-Fusion-Features.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/wapopartners/Skeleton-PageBuilder-Fusion-Features/issues"
  },
  "homepage": "https://github.com/wapopartners/Skeleton-PageBuilder-Fusion-Features#readme"
}
